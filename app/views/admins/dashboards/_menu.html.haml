      = link_to additional_charge_admins_tickets_path, class: "list-group-item #{'active' if params[:action] == 'additional_charge' } #{can?(:additional_charge, (TicketEstimation and AdditionalCharge)) ? '' : 'disabled'}" do
        %i.fa.fa-money
        Additional Charge
      = link_to dispatch_method_admins_tickets_path, class: "list-group-item #{'active' if params[:action] == 'dispatch_method' } #{can?(:dispatch_method, (Invoice and DispatchMethod)) ? '' : 'disabled'}" do
        %i.fa.fa-certificate
        Dispatch Method
      = link_to payment_item_admins_tickets_path, class: "list-group-item #{'active' if params[:action] == 'payment_item' } #{can?(:payment_item, PaymentItem) ? '' : 'disabled'}" do
        %i.fa.fa-arrows
        Terminate payment item
      = link_to payment_term_admins_tickets_path, class: "list-group-item #{'active' if params[:action] == 'payment_term' } #{can?(:payment_term, PaymentTerm) ? '' : 'disabled'}" do
        %i.fa.fa-bullseye
        Payment term
      = link_to tax_admins_tickets_path, class: "list-group-item #{'active' if params[:action] == 'tax' } #{can?(:tax, Tax) ? '' : 'disabled'}" do
        %i.fa.fa-link
        Tax
      = link_to annexture_admins_tickets_path, class: "list-group-item #{'active' if params[:action] == 'annexture' } #{can?(:annexture, Documents::BrandDocument) ? '' : 'disabled'}" do
        %i.fa.fa-link
        Annextures

    .panel-footer
      = link_to "Tickets", admins_tickets_path

  = collapse collapse_type: "#{controller_name == 'inventories' ? 'primary' : 'default'}", labelledby: "labelledby_inventories", header_content: content_tag(:span, "", class: "icon-list-alt")+"Inventories", collapse_link: "collapse_link_inventories" do
    .list-group.small_scale_margin0
      = link_to inventory_brand_admins_inventories_path, class: "list-group-item #{'active' if params[:action] == 'inventory_brand' } #{can?(:inventory_brand, InventoryCategory1) ? '' : 'disabled'}" do
