= collapse_wrapper collapse_id: "sidebar-nav" do
  = collapse collapse_type: "#{controller_name == 'organizations' ? 'primary' : 'default'}", labelledby: "labelledby_organizations", header_content: content_tag(:span, "", class: "icon-sitemap")+"Organizations", collapse_link: "collapse_link_organizations" do
    .list-group.small_scale_margin0
      = link_to country_admins_organizations_path, class: "list-group-item #{'active' if params[:action] == 'country' } #{can?(:country, ProductSoldCountry) ? '' : 'disabled'}" do
        %i.fa.fa-flag
        Country
      = link_to regional_support_center_admins_organizations_path, class: "list-group-item #{'active' if params[:action] == 'regional_support_center' } #{can?(:regional_support_center, (TaskAction and RegionalSupportCenter)) ? '' : 'disabled'}" do
        %i.fa.fa-sitemap
        Regional
      = link_to sla_admins_organizations_path, class: "list-group-item #{'active' if params[:action] == 'sla' } #{can?(:sla, SlaTime) ? '' : 'disabled'}" do
        %i.fa.fa-clock-o
        SLA
      = link_to store_and_branch_admins_organizations_path, class: "list-group-item #{'active' if params[:action] == 'store_and_branch' } #{can?(:sla, SlaTime) ? '' : 'disabled'}" do
        %i.fa.fa-clock-o
        Store & branch

      = link_to industry_type_admins_organizations_path, class: "list-group-item #{'active' if params[:action] == 'industry_type' } #{can?(:industry_type, IndustryType) ? '' : 'disabled'}" do
        %i.fa.fa-adjust
        Industry Type

  = collapse collapse_type: "#{controller_name == 'employees' ? 'primary' : 'default'}", labelledby: "labelledby_employees", header_content: content_tag(:span, "", class: "icon-group")+"Employees", collapse_link: "collapse_link_employees" do
    .list-group.small_scale_margin0
      = link_to sbu_admins_employees_path, class: "list-group-item #{'active' if params[:action] == 'sbu' } #{can?(:sbu, Sbu) ? '' : 'disabled'}" do
        %i.fa.fa-asterisk
        Sbu



  = collapse collapse_type: "#{controller_name == 'users' ? 'primary' : 'default'}", labelledby: "labelledby_users", header_content: content_tag(:span, "", class: "icon-group")+"Users", collapse_link: "collapse_link_users" do
    .list-group.small_scale_margin0
      = link_to title_admins_users_path, class: "list-group-item #{'active' if params[:action] == 'title' } #{can?(:title, MstTitle) ? '' : 'disabled'}" do
        %i.fa.fa-users
        Title

  = collapse collapse_type: "#{controller_name == 'tickets' ? 'primary' : 'default'}", labelledby: "labelledby_tickets", header_content: content_tag(:span, "", class: "icon-tty")+"Tickets", collapse_link: "collapse_link_tickets" do
    .list-group.small_scale_margin0
      = link_to brands_and_category_admins_tickets_path, class: "list-group-item #{'active' if params[:action] == 'brands_and_category' } #{can?(:brands_and_category, SparePartDescription) ? '' : 'disabled'}" do
        %i.icon-paper
        Brand
      = link_to brand_category1_admins_tickets_path, class: "list-group-item #{'active' if params[:action] == 'brand_category1' }" do
        %i.icon-paper
        Product Category1
      = link_to brand_category2_admins_tickets_path, class: "list-group-item #{'active' if params[:action] == 'brand_category2' }" do
        %i.icon-paper
        Product Category2
      = link_to brand_category3_admins_tickets_path, class: "list-group-item #{'active' if params[:action] == 'brand_category3' }" do
        %i.icon-paper
        Product Category3
      = link_to spare_part_description_admins_tickets_path, class: "list-group-item #{'active' if params[:action] == 'spare_part_description' }" do
        %i.fa.fa-file-text-o
        Spare Part Description
      = link_to reason_admins_tickets_path, class: "list-group-item #{'active' if params[:action] == 'reason' } #{can?(:reason, Reason) ? '' : 'disabled'}" do
