      = link_to reason_admins_inventories_path, class: "list-group-item #{'active' if params[:action] == 'inventory_reason' } #{can?(:reason, InventoryReason) ? '' : 'disabled'}" do
        %i.fa.fa-comments
        Reason
    / .panel-footer
    /   = link_to "Inventories", admins_inventories_path
