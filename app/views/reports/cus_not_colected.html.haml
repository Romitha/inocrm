- tickets = @tickets
%h3 Items Not Collected By Customer Report
.row
  .col-md-1
    %strong No
  .col-md-3
    %strong Part order date
  .col-md-2
    %strong Ticket No
  .col-md-3
    %strong Customer Name
  .col-md-3
    %strong Model No
- num = 0
%hr
- tickets.each_with_index do |ticket, index|
  - conditions = ticket.job_finished && ticket.inhouse_type_select == "IN"
  -if conditions
    .row
      .col-md-1
        - if params[:page]
          = (index+1)+ 10*(params[:page].to_i-1)
        - else
          - num= num+1
          = (num)
      .col-md-3= ticket.job_finished_at.to_date.try :strftime, INOCRM_CONFIG["short_date_format"]
      .col-md-2= ticket.support_ticket_no
      .col-md-3= ticket.customer_name
      .col-md-3= ticket.products.first.model_no

    %hr